VOVKPLCPROJECT MultiBlockWithSymbols
VERSION 3.0

MEMORY
M 448-703
I 64-127
Q 128-191
T 704-1023
END_MEMORY

SYMBOLS
    start_btn : bit : X0.0
    stop_btn : bit : X0.1
    motor : bit : Y0.0
    error_led : bit : Y0.1
END_SYMBOLS

PROGRAM Main
    BLOCK PLCASM_sample LANG=PLCASM
u8.readBit start_btn
u8.readBit stop_btn
u8.not
u8.and
jump_if_not skip_set
u8.writeBitOn motor
skip_set:
    END_BLOCK

    BLOCK STL_sample LANG=STL
A stop_btn
R motor
    END_BLOCK

    BLOCK Ladder_sample LANG=LADDER
{
  "comment": "Network 1",
  "nodes": [
    { "id": "gzFIdmMFsKIZe", "x": 2, "y": 0, "type": "coil", "symbol": "error_led", "inverted": false },
    { "id": "dkK9jduUKYAnf", "x": 0, "y": 0, "type": "contact", "symbol": "start_btn", "inverted": false, "trigger": "normal" },
    { "id": "yUcYfCm0pKygR", "x": 1, "y": 0, "type": "contact", "symbol": "stop_btn", "inverted": false, "trigger": "normal" }
  ],
  "connections": [
    { "sources": [ "dkK9jduUKYAnf" ], "destinations": [ "yUcYfCm0pKygR" ] }, 
    { "sources": [ "yUcYfCm0pKygR" ], "destinations": [ "gzFIdmMFsKIZe" ] }
  ]
}
    END_BLOCK
END_PROGRAM
